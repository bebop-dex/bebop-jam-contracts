{
  "name": "bebop-jam-contracts",
  "version": "2.0.0",
  "description": "Bebop JAM",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:jam": "npx hardhat test ./test/hardhat/JamOrders.test.ts",
    "test:blend": "npx hardhat test ./test/hardhat/BlendOrders.test.ts",
    "deploy": "npx hardhat deploy",
    "deploySolver": "npx hardhat deploySolver",
    "deploy:polygon": "npx hardhat --network polygon deploy",
    "deploy:arbitrum": "npx hardhat --network arbitrum deploy",
    "deploy:base": "npx hardhat --network base deploy",
    "deploy:optimism": "npx hardhat --network optimism deploy",
    "deploy:taiko": "npx hardhat --network taiko deploy",
    "deploy:bsc": "npx hardhat --network bsc deploy",
    "deploy:ethereum": "npx hardhat --network ethereum deploy",
    "deploy:zksync": "npx hardhat deploy-zksync --network zkSyncMainnet --script deploy/deployZkSync.ts",
    "deploy:blast": "npx hardhat --network blast deploy",
    "deploy:scroll": "npx hardhat --network scroll deploy",
    "deploy:mode": "npx hardhat --network mode deploy",
    "deploy:megaeth": "npx hardhat --network megaeth deploy",
    "verify:polygon": "npx hardhat verify --network polygon --constructor-args tasks/arguments/args.ts",
    "verify:arbitrum": "npx hardhat verify --network arbitrum --constructor-args tasks/arguments/args.ts",
    "verify:base": "npx hardhat verify --network base --constructor-args tasks/arguments/args.ts",
    "verify:optimism": "npx hardhat verify --network optimism --constructor-args tasks/arguments/args.ts",
    "verify:taiko": "npx hardhat verify --network taiko --constructor-args tasks/arguments/args.ts",
    "verify:bsc": "npx hardhat verify --network bsc --constructor-args tasks/arguments/args.ts",
    "verify:ethereum": "npx hardhat verify --network ethereum --constructor-args tasks/arguments/args.ts",
    "verify:zksync": "HARDHAT_NETWORK=zkSyncMainnet npx ts-node tasks/verifyZksync.ts",
    "verify:blast": "npx hardhat verify --network blast --constructor-args tasks/arguments/args.ts",
    "verify:scroll": "npx hardhat verify --network scroll --constructor-args tasks/arguments/args.ts",
    "verify:mode": "npx hardhat verify --network mode --constructor-args tasks/arguments/args.ts",
    "flatten-contracts": "sol-merger \"./src/*.sol\" ./flatten"
  },
  "author": "<info@bebop.xyz>",
  "devDependencies": {
    "@1inch/permit-signed-approvals-utils": "^1.4.8",
    "@matterlabs/hardhat-zksync-deploy": "^0.7.0",
    "@matterlabs/hardhat-zksync-node": "^0.0.2",
    "@matterlabs/hardhat-zksync-solc": "^1.1.2",
    "@matterlabs/hardhat-zksync-verify": "^0.5.0",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.35",
    "@uniswap/permit2-sdk": "^1.2.0",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.6.6",
    "hardhat": "^2.17.0",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-preprocessor": "^0.1.4",
    "sol-merger": "^4.4.1",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4",
    "zksync-ethers": "^5.3.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2"
  }
}
